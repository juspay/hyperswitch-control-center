{
  "presets": ["@babel/preset-env", "@babel/preset-react"],
  "plugins": [
    [
      "module-resolver",
      {
        "root": ["./src"],
        "alias": {
          "test": "./test",
          "underscore": "lodash"
        }
      }
    ],
    "transform-class-properties",
    "istanbul",
    "@babel/plugin-transform-async-to-generator"
  ]
}
